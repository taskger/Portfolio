// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:app.db"
}

model Skill {
  id          Int       @id @default(autoincrement())
  name        String
  icon        String
  createDate  DateTime @default(now())
}
model Project {
  id          Int       @id @default(autoincrement())
  name        String
  tool        String
  image       String
  githublink  String
  figmalink   String
  demolink    String
  createDate  DateTime @default(now())
}
model User {
  id          Int       @id @default(autoincrement())
  user        String
  pass        String
  createDate  DateTime @default(now())
  loginLast   DateTime
}